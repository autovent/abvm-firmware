{
  "build": {
    "core": "stm32",
    "cpu": "cortex-m3",
    "extra_flags": "-DSTM32F1 -DSTM32F103xB",
    "f_cpu": "72000000L",
    "libopencm3": {
      "ldscript": "stm32f103xB.ld"
    },
    "mcu": "stm32f103rbtx"
  },
  "debug": {
    "default_tools": [
      "stlink"
    ],
    "jlink_device": "STM32F103RB",
    "openocd_target": "stm32f1x",
    "svd_path": "STM32F103xx.svd"
  },
  "frameworks": [
    "stm32cube"
  ],
  "name": "BVM Squeezer",
  "upload": {
    "maximum_ram_size": 20480,
    "maximum_size": 65536,
    "protocol": "stlink",
    "protocols": [
      "stlink"
    ]
  },
  "vendor": "AutoVENT",
  "url": "autovent.org"
}
